<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.n52.shared.serializable.pojos">
  <class name="TimeseriesFeed" table="feeding_timeseries" >
      <id name="id">
         <generator class="sequence">
            <param name="sequence">sensor_id_seq</param>
         </generator>
      </id>
      <property name="sesId" type="string"/>
      <property name="lastUpdate" type="calendar"/>
      <property name="usedCounter" type="long"/>
      <property name="updateInterval" type="long"/>
      <property name="active" type="boolean"/>
      <property name="inUse" type="integer"/>
      <property name="feedingMetadata" type="org.n52.shared.serializable.pojos.FeedingMetadata" />
   </class>
</hibernate-mapping>