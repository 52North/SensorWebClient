<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.n52.shared.serializable.pojos">
  <class name="Subscription" table="sesclient_subscription" >
      <id name="id" column="ID">
         <generator class="native"/>
      </id>
      <property name="userID" type="integer"/>
      <property name="ruleID" type="integer"/>
      <property name="subscriptionID" type="string"/>
      <property name="medium" type="string"/>
      <property name="format" type="string"/>
      <property name="active" type="boolean"/>
   </class>
</hibernate-mapping>