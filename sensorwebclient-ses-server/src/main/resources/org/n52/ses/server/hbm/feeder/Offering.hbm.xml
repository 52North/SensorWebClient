<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.n52.server.ses.feeder.hibernate">
  <class name="Offering" table="feeder_offering" >
      <id name="offeringID">
         <generator class="sequence">
            <param name="sequence">offering_id_seq</param>
         </generator>
      </id>
      <property name="name" type="string"/>
      <many-to-one name="sensor" lazy="false" column="sensor_id" class="Sensor"/>
      <set name="observedProperties">
        <key column="offering_id"/>
        <one-to-many class="ObservedProperty"/>
      </set>
   </class>
</hibernate-mapping>