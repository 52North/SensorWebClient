<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.n52.server.ses.feeder.hibernate">
  <class name="ObservedProperty" table="feeder_observed_property" >
      <id name="obsPropId">
         <generator class="sequence">
            <param name="sequence">obs_Prop_id_seq</param>
         </generator>
      </id>
      <property name="name" type="string"/>
      <many-to-one name="offering" lazy="false" column="offering_id" class="Offering"/>
   </class>
</hibernate-mapping>