<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.n52.server.ses.feeder.hibernate">
  <class name="SensorToFeed" table="feeder_sensor" >
      <id name="id">
         <generator class="sequence">
            <param name="sequence">sensor_id_seq</param>
         </generator>
      </id>
      <property name="procedure" type="string"/>
      <property name="offering" type="string"/>
      <property name="phenomenon" type="string"/>
      <property name="featureOfInterest" type="string"/>
      <property name="serviceURL" type="string"/>
      <property name="sesId" type="string"/>
      <property name="lastUpdate" type="calendar"/>
      <property name="usedCounter" type="long"/>
      <property name="updateInterval" type="long"/>
   </class>
</hibernate-mapping>