<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.n52.shared.serializable.pojos">
  <class name="TimeseriesMetadata" table="timeseries_metadata" >
      <id name="id">
         <generator class="native"/>
      </id>
      <property name="serviceUrl" type="string"/>
      <property name="procedure" type="string"/>
      <property name="offering" type="string"/>
      <property name="phenomenon" type="string"/>
      <property name="featureOfInterest" type="string"/>
      <property name="timeseriesId" type="string" unique="true" />
   </class>
</hibernate-mapping>