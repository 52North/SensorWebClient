<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.n52.ses.server.feeder.hibernate">
  <class name="SOS" table="feeder_sos" >
      <id name="id">
         <generator class="sequence">
            <param name="sequence">sos_id_seq</param>
         </generator>
      </id>
      <property name="url" type="string"></property>
      <set name="sensors">
         <key column="sos_id"/>
         <one-to-many class="Sensor"/>
      </set>
   </class>
</hibernate-mapping>