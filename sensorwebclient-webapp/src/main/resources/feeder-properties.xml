<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<!-- URL to the SES and some other configurations for the SES -->
	<entry key="ses_url">${ses.service.url}</entry>
	
	<!-- Period to start to collect the sensorML documents in milliseconds -->
	<entry key="capabilities_task_period">${feeder.capabilities.task.period}</entry>
	
	<!-- Period to start to collect the new observations in milliseconds -->
	<entry key="observations_task_period">${feeder.observation.task.period}</entry>
	
    <!-- NoData wildcards as comma separated values -->
	<entry key="nodata_values">${feeder.nodata.values}</entry>
	
	
	<!-- ################################################## 
		        ADVANCED CONFIGURATION PARAMETERS
	     ################################################## -->
	     
	<entry key="ses_default_topic_dialect">${feeder.ses.default.topic.dialect}</entry>
	<entry key="ses_register_publisher_lifetime">${feeder.ses.register.publisher.lifetime}</entry>
	<entry key="ses_register_publisher_endpoint">${feeder.ses.register.publisher.endpoint}</entry>
	<entry key="ses_default_topic">${feeder.ses.default.topic}</entry>

	<!-- Minimum update time for new observations in milliseconds -->
	<entry key="update_observation_period">${feeder.update.observation.period}</entry>
    <!-- Prohibit procedure names as comma separated value -->
    <entry key="prohibit_procedure_names">${feeder.prohibit.procedure.name}</entry>
    <!-- Start timestamp in seconds before actual time -->
    <entry key="start_timestamp">${feeder.start.timestamp}</entry>
    <!-- Sleep time between two observations send to SES in milliseconds -->
    <entry key="sleep_time_observation">${feeder.sleep.time.observation}</entry>
    <!-- Take just the youngest new observation an send it to the ses -->
    <entry key="only_youngest_observation">${feeder.only.youngest.observation}</entry>
	
</properties>