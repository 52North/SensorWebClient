<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="client">
	<inherits name='com.google.gwt.user.User' />
    <inherits name="com.google.gwt.i18n.I18N" />
	<inherits name='org.eesgmbh.gimv.Gimv' />
	<inherits name="com.smartgwt.SmartGwt" />
	<inherits name="com.smartgwt.tools.SmartGwtTools" />
	<inherits name="org.gwtopenmaps.openlayers.OpenLayers" />
	<inherits name="org.n52.SensorWebClient_Ext" />
	 
    <extend-property name="locale" values="en" />
    <extend-property name="locale" values="de" />
    <set-property-fallback name="locale" value="en" />
    <set-property name='user.agent' value="gecko1_8" />
    <set-configuration-property name="locale.useragent" value="true" />
    <set-configuration-property name="locale.searchorder" value="queryparam,useragent" />
	 
    <entry-point class='org.n52.client.Application' />
	 
	<source path='client' />
	<source path='shared' />
	<source path='server' />

</module>
